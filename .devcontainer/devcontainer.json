{
    "name": "RailsApp Docker",
    "dockerComposeFile": "../docker-compose.yml",
    "service": "app",
    "workspaceFolder": "/var/lib/app",
    "shutdownAction": "stopCompose",
    "appPort": 3000,
    "extensions": [
        "rebornix.ruby",
        "eamodio.gitlens",
        "wakatime.vscode-wakatime",
        "kaiwood.endwise"
    ],
    "settings": {
        "terminal.integrated.shell.linux": "/bin/bash"
    }
}
