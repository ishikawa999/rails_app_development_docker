{
    "name": "AppName",
    "dockerComposeFile": "../docker-compose.yml",
    "service": "app",
    "workspaceFolder": "/var/lib/app",
    "shutdownAction": "stopCompose",
    "appPort": [8000, 3035],
    "extensions": [
        "rebornix.ruby",
        "eamodio.gitlens",
        "kaiwood.endwise",
        "shuworks.vscode-table-formatter",
        "humao.rest-client"
    ],
    "settings": {
        "terminal.integrated.shell.linux": "/bin/bash"
    }
}
